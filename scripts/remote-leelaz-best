#!/usr/bin/env bash

#Script for executing leelaz with its best network remotely.

set -eu
DIR="$( cd "$( dirname "${BASH_SOURCE[0]}" )" >/dev/null 2>&1 && pwd )"
source "${DIR}/../config.sh"
gcloud compute ssh "$INSTANCE_NAME" --zone "$ZONE" --command "/leela/leelaz -g -w /leela/best-network.gz -b 0 -t 16"
